{
  "name": "srcjs",
  "version": "0.0.1",
  "description": "A TypeScript wrapper for the SRC API",
  "license": "MIT",
  "author": "Brady Shea",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "keywords": [
    "speedrun"
  ],
  "scripts": {
    "build": "tsup",
    "test": "node dist/src/test.js"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
